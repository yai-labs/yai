{
  "baseline_id": "baseline-quarantine",
  "baseline_version": 1,
  "policy": {
    "content_ops": {
      "mode": "deny_or_quarantine_on_violation",
      "require_provenance": true,
      "require_policy_basis": true,
      "require_contestability_for_high_impact": true,
      "allowed_scopes": ["sandbox"],
      "high_impact_actions": ["content.moderation.enforce", "content.visibility.change"],
      "on_violation": {
        "provenance_missing": { "outcome": "deny", "reason_code": "PROVENANCE_MISSING" },
        "policy_basis_missing": { "outcome": "deny", "reason_code": "POLICY_BASIS_MISSING" },
        "contestability_missing": { "outcome": "deny", "reason_code": "CONTESTABILITY_MISSING" },
        "scope_not_authorized": { "outcome": "deny", "reason_code": "SCOPE_NOT_AUTHORIZED" },
        "arming_missing": { "outcome": "deny", "reason_code": "ARMING_REQUIRED" }
      }
    }
  }
}
