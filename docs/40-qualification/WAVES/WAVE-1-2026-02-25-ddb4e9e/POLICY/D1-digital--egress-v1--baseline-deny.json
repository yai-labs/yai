{
  "baseline_id": "baseline-deny",
  "baseline_version": 1,
  "policy": {
    "network_egress": {
      "mode": "deny_unless_allowlisted",
      "allowlist": {
        "cidr": ["10.0.0.0/8", "192.168.0.0/16"],
        "hosts": []
      },
      "allowed_ports": [53, 80, 443],
      "default_outcome": "deny",
      "reason_code_on_deny": "EGRESS_DEST_NOT_CONTRACTED"
    },
    "thresholds": {
      "egress_attempts_per_minute": 60,
      "egress_burst": 10
    }
  }
}
