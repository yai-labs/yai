{
  "required_fields": [
    "timestamp",
    "ws_id",
    "trace_id",
    "event.type",
    "event.source",
    "principal.id",
    "principal.role",
    "target.pipeline_id",
    "target.run_id",
    "target.dataset_ref",
    "target.params_hash",
    "target.lock_hash",
    "target.dst.path",
    "target.dst.url",
    "decision.outcome",
    "decision.reason_code",
    "decision.baseline_id",
    "decision.baseline_hash",
    "enforcement.result",
    "metrics.time_to_contain_ms",
    "metrics.target_reachable",
    "metrics.run_started",
    "metrics.outputs_persisted",
    "metrics.bytes_written",
    "metrics.artifacts_delta"
  ],
  "notes": {
    "params_hash": "Stable sha256 hash of params.json.",
    "lock_hash": "sha256 hash of params.lock, or null when missing.",
    "outputs_persisted": "Must be false under deny/quarantine.",
    "artifacts_delta": "Black-box count delta on artifact-store files."
  }
}
